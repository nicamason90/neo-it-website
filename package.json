{
  "name": "neo-it-nextjs",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "framer-motion": "^10.16.4",
    "next": "^15.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "@next/bundle-analyzer": "^14.2.0",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
